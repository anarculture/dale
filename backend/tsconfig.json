{
  "compilerOptions": {
    "target": "ES2021",
    "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
   "typeRoots": [
    "./node_modules/@types",
    "./src/types"
  ] ,
    "types": [
      "node",
      "express",
      "express-session",
      "jest"
    ]
  },
  "include": [
    "src",
    "src/types"
  ]
}
